<?xml version="1.0"?>
<extension key="at.greenpeace.gpapi" type="module">
  <file>gpapi</file>
  <name>GP API Extension</name>
  <description>Interface to OSF, Versandtool, etc.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/at.greenpeace.uimods</url>
    <url desc="Documentation">https://github.com/systopia/at.greenpeace.uimods</url>
    <url desc="Support">https://github.com/systopia/at.greenpeace.uimods/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-07-23</releaseDate>
  <version>1.9.3</version>
  <develStage>stable</develStage>
  <requires>
    <ext>de.systopia.xcm</ext>
    <ext>com.cividesk.normalize</ext>
    <ext>de.systopia.contract</ext>
  </requires>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Gpapi</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
